{
  "name": "mastorscdn",
  "version": "1.0.0",
  "private": true,
  "description": "MastorsCDN is a lightweight SCSS mixin and CSS utility library designed for CDN delivery and static usage.",
  "keywords": [
    "css",
    "scss",
    "sass",
    "cdn",
    "utility",
    "mixin",
    "frontend",
    "static"
  ],
  "author": "KEHEM-IT",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/KEHEM-IT/MastorsCDN"
  },
  "scripts": {
    "scss:build": "sass --load-path=node_modules styles/mastorscdn.scss styles/mastorscdn.css",
    "scss:watch": "sass --watch --load-path=node_modules styles/mastorscdn.scss:styles/mastorscdn.css",
    "ts:build": "tsc",
    "ts:watch": "tsc --watch",
    "build": "npm run scss:build && npm run ts:build",
    "watch": "npm run scss:watch & npm run ts:watch",
    "dev": "npm run watch"
  },
  "devDependencies": {
    "concurrently": "^9.2.1",
    "sass": "^1.69.0",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "mastors-fluider": "^1.5.5",
    "mastors-gridder": "^1.0.0"
  }
}
